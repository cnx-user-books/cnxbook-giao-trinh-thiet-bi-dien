                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ size 10{ϕ \( f \) `= {  { size 10{a rSup { size 8{2} } }}  over  { size 12{b rSup { size 8{2} } } } }  \[  \( 1+ {  { size 12{a} }  over  { size 12{b} } }  \) + \( 1 -  {  { size 12{b} }  over  { size 12{a} } }  \)  "." "ln" \( 1 -  {  { size 12{b} }  over  { size 12{a} } }  \)  \] }} {}</m:annotation>
